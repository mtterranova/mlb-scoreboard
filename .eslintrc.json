{
	"extends": ["airbnb", "prettier", "prettier/react"],
	"plugins": ["prettier", "react"],
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"jsx-a11y/href-no-hash": "off",
	"jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
}
